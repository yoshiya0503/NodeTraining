extends layout

block content
    script(src='http://ajax.googleapis.com/ajax/libs/jquery/1.10.0/jquery.js')
    script(src='/socket.io/socket.io.js')
    script.
        $(function () {
            var socket = io.connect();
            socket.on('access', function (data) {
                $('#count').text(data);
            });
        });
